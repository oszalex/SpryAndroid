create_table {
  name 'account'
  fields {
    number 'string'
    secret 'string'
  }
}

create_table {
  name 'event'
  fields {
    user 'string'
    start_time 'long'
    description 'string'
    duration 'integer'
    max_attending 'integer'
    min_attending 'integer'
    price 'double'
    is_public 'boolean'
  }
}

create_table {
  name 'keyword'
  fields {
    text 'string'
    event_id 'long'
  }
}